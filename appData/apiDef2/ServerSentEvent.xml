    <dnName>ServerSentEvent</dnName>
    <axName>ChilkatServerSentEvent</axName>
    <description>Represents a server-sent event (SSE) received from an HTTP text/event-stream.</description>
    <methods>
        <method name="LoadEvent" axId="104" ax="1" cpp="1" dn="1" status="1">
            <gType>bool</gType>
            <descrip>Loads the multi-line event text into this object.  For example, the ARG1 for a Firebase event might look like this:
&lt;pre&gt;
event: put
data: {"path": "/c", "data": {"foo": true, "bar": false}}
&lt;/pre&gt;</descrip>
            <iversion>9.5.0.58</iversion>
            <args>
                <arg>
                    <argName>eventText</argName>
                    <cppType>const char *</cppType>
                    <dnType>string</dnType>
                    <axType>string</axType>
                    <objcName>eventText</objcName>
                </arg>
            </args>
            <links2>
                <link linkType="example">
                    <url>firebase_eventStream.asp</url>
                    <text>Firebase Receive Server-Sent Events (text/event-stream)</text>
                </link>
            </links2>
            <args2>
                <arg name="eventText" type="string" objcName="eventText" />
            </args2>
        </method>
    </methods>
    <properties>
        <property name="Data" read="1" ax="1" cpp="1" dn="1" axId="101">
            <gType>string</gType>
            <descrip>The data for the server-side event.  (If the "data" field was empty, then this will be empty.)</descrip>
            <iversion>9.5.0.58</iversion>
            <links2 />
            <args2 />
        </property>
        <property name="EventName" read="1" ax="1" cpp="1" dn="1" axId="100">
