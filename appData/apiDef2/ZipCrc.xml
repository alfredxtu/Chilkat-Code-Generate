                    <argName>data</argName>
                    <cppType>CkByteData &amp;</cppType>
                    <dnType>bytes</dnType>
                    <axType>bytes</axType>
                    <objcName>data</objcName>
                </arg>
            </args>
            <args2>
                <arg name="data" type="bytes" objcName="data" />
