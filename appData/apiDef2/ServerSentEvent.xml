            <iversion>9.5.0.58</iversion>
            <links2 />
            <args2 />
        </property>
        <property name="LastEventId" read="1" ax="1" cpp="1" dn="1" axId="102">
            <gType>string</gType>
            <descrip>The content of the "id" field, if present.</descrip>
            <iversion>9.5.0.58</iversion>
            <links2 />
            <args2 />
        </property>
        <property name="Retry" read="1" ax="1" cpp="1" dn="1" axId="103">
